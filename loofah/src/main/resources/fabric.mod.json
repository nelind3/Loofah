{
  "schemaVersion": 1,
  "id": "loofah",
  "version": "${version}",
  "name": "Loofah",
  "description": "A SpongeAPI implementation for the Fabric ecosystem",
  "authors": [
    "Nelind"
  ],
  "contact": {
    "homepage": "https://www.nelind.dk",
    "sources": "https://github.com/nelind3/Loofah"
  },
  "license": "MIT",
  "icon": "assets/loofah/icon.png",
  "environment": "*",
  "entrypoints": {
    "preLaunch": [
      "dk.nelind.loofah.applaunch.AppLaunchMain"
    ],
    "main": [
      "dk.nelind.loofah.Loofah"
    ],
    "client": [
      "dk.nelind.loofah.Loofah"
    ]
  },
  "accessWidener" : "common.accesswidener",
  "mixins": [
    "mixins.loofah.api.json",
    "mixins.loofah.core.json",
    "mixins.loofah.inventory.json",
    "mixins.loofah.tracker.json",
    "mixins.sponge.api.json",
    "mixins.sponge.accessors.json",
    "mixins.sponge.concurrent.json",
    "mixins.sponge.core.json",
    "mixins.sponge.entityactivation.json",
    "mixins.sponge.exploit.json",
    "mixins.sponge.inventory.json",
    "mixins.sponge.ipforward.json",
    "mixins.sponge.movementcheck.json",
    "mixins.sponge.optimization.json",
    "mixins.sponge.tracker.json"
  ],
  "depends": {
    "java": ">=${javaVersion}",
    "minecraft": "~${minecraftVersion}",
    "fabricloader": ">=${fabricLoaderVersion}",
    "fabric-lifecycle-events-v1": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}